{
  "name": "delphirtl",
  "version": "1.1.3.1",
  "description": "RTL functions from Delphi",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/rtl.js",
    "./dateutils": "./dist/dateutils.js",
    "./logutils": "./dist/logutils.js",
    "./rtl": "./dist/rtl.js",
    "./sysutils": "./dist/sysutils.js",
    "./ArrayUtils": "./dist/arrayutils.js",
    "./DateUtils": "./dist/dateutils.js",
    "./LogUtils": "./dist/logutils.js",
    "./SysUtils": "./dist/sysutils.js",
    "./Messaging": "./dist/messaging.js",
    "./messaging": "./dist/messaging.js",
    "./ReflectUtils": "./dist/reflectutils.js",
    "./PerformanceUtils": "./dist/performanceutils.js"
  },
  "typesVersionsComments": "ArrayUtils mapped to arrayutils, and so on. ",
  "typesVersions": {
    "*": {
      ".": [
        "dist/rtl"
      ],
      "ArrayUtils": [
        "dist/arrayutils"
      ],
      "DateUtils": [
        "dist/dateutils"
      ],
      "LogUtils": [
        "dist/logutils"
      ],
      "SysUtils": [
        "dist/sysutils"
      ],
      "ReflectUtils": [
        "dist/reflectutils"
      ],
      "PerformanceUtils": [
        "dist/performanceutils"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepare": "del dist /s /q && tsc --build",
    "prepare-uncommented": "// && npm-dts -o dist\\index.d.ts generate",
    "build": "tsc",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chuacw/delphirtl.git"
  },
  "keywords": [
    "delphi",
    "rtl"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "chuacw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chuacw/delphirtl/issues"
  },
  "homepage": "https://github.com/chuacw/delphirtl#readme",
  "dependencies": {
    "@types/node": "^17.0.25",
    "delphirtl": "file:",
    "ethers": "^5.6.6",
    "express": "^4.18.2",
    "http-terminator": "^3.2.0",
    "json-rpc-2.0": "^1.4.1",
    "node-fetch": "^3.3.0",
    "superstruct": "^0.16.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.1",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.8.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=17.5.0"
  }
}
